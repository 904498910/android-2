<?xml version="1.0" encoding="UTF-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="MainFeedText">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">14dp</item>
    </style>
    <style name="ListItemText" parent="@android:style/TextAppearance.Holo.Large">
        <item name="android:textColor">@color/left_text_selector</item>
        <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
        <item name="android:drawablePadding">5dp</item>
    </style>
    <style name="AutocompleteResultText" parent="@android:style/TextAppearance.Holo.Large">
        <item name="android:textColor">#54595f</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="AutocompleteDetailText" parent="@android:style/TextAppearance.Holo.Small">
        <item name="android:textColor">#7c8594</item>
        <item name="android:maxLines">2</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="PullToRefreshMainText" parent="@android:style/TextAppearance.Holo.Large">
        <item name="android:singleLine">true</item>
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="PullToRefreshSubText" parent="@android:style/TextAppearance.Holo.Small">
        <item name="android:singleLine">true</item>
        <item name="android:textAppearance">?android:attr/textAppearanceSmall</item>
    </style>
</resources>